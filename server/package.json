{
  "name": "e-commerce-server",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.17.1"
  },
  "scripts": {
    "build": "babel ./server.js --out-dir ./dist",
    "start": "node ./dist/server",
    "start:dev": "NODE_ENV=development npm-run-all build start",
    "start:watch": "nodemon --exec babel-node ./server.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
